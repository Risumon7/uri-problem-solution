#include <stdio.h>

int main()
{
    int years,months,days;
    scanf("%d", &days);
    years = days / 365;
    printf("%d ano(s)\n", years);
    days = days - (years * 365);
    months = days / 30;
    printf("%d mes(es)\n", months);
    days = days - (months * 30);
    printf("%d dia(s)\n", days);
    return 0;
}
